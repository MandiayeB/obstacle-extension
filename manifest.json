{
    "manifest_version": 3,
    "name": "Obstacle extension",
    "version": "1.0.0",
    "description": "Send notifications to Obstacle users.",
    "icons": {
        "128": "./icons/icon128.png",
        "48": "./icons/icon48.png",
        "16": "./icons/icon16.png"
    },
    "action": {
        "default_icon": "./icons/icon16.png",
        "default_popup": "./public/popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}